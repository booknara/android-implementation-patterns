<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.booknara.patterns.databinding">

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name="com.booknara.android.apps.patterns.databinding.MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>

    <activity android:name="com.booknara.android.apps.patterns.databinding.TwoWayExampleActivity" />
    <activity android:name="com.booknara.android.apps.patterns.databinding.ViewModExampleActivity" />
    <activity android:name="com.booknara.android.apps.patterns.databinding.ObservableFieldsActivity" />
    <activity android:name="com.booknara.android.apps.patterns.databinding.SimpleExampleActivity" />
  </application>

</manifest>
